{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ], 
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": [
            true,
            "ignore-blank-lines",
            "ignore-case",
            "locale-compare",
            // "match-declaration-order",
            "shorthand-first"
        ]
    }
}